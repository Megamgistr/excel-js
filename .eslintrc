{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "quotes":"off",
        "arrow-parens":"off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "object-curly-spacing":"off",
        "indent":"off",
        "operator-linebreak":"off",
        "one-var":"off"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
}